						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "dt_ca_tbl_ticket",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ext_ca_call_req_complement",
